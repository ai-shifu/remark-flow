{
  "name": "remark-flow",
  "version": "1.0.0-alpha.8",
  "description": "Remark plugins for custom button and variable syntax transformation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --build --clean && tsc --build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js --report-unused-disable-directives --max-warnings 20",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "remark",
    "remark-plugin",
    "markdown",
    "custom-variable"
  ],
  "author": "remark-flow contributors",
  "homepage": "https://github.com/ai-shifu/remark-flow#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-shifu/remark-flow.git"
  },
  "bugs": {
    "url": "https://github.com/ai-shifu/remark-flow/issues"
  },
  "license": "MIT",
  "dependencies": {
    "unist": "^0.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/unist": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "remark": "^15.0.0",
    "remark-parse": "^11.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "unified": "^11.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
